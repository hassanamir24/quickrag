{
  "name": "quickrag",
  "version": "1.0.2",
  "type": "module",
  "description": "A fast and flexible RAG tool for indexing and querying documents",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir=dist --target=bun",
